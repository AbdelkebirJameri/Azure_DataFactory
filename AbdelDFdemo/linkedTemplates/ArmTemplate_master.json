{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "AbdelDFdemo"
		},
		"Inputblobb_connectionString": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « connectionString » de « Inputblobb »"
		},
		"OutputSql_password": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « password » de « OutputSql »"
		},
		"OutputSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tcp:abdeldemo.database.windows.net,1433"
		},
		"OutputSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "abde_demo"
		},
		"OutputSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "abdel"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AbdelDFdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Inputblobb_connectionString": {
						"value": "[parameters('Inputblobb_connectionString')]"
					},
					"OutputSql_password": {
						"value": "[parameters('OutputSql_password')]"
					},
					"OutputSql_properties_typeProperties_server": {
						"value": "[parameters('OutputSql_properties_typeProperties_server')]"
					},
					"OutputSql_properties_typeProperties_database": {
						"value": "[parameters('OutputSql_properties_typeProperties_database')]"
					},
					"OutputSql_properties_typeProperties_userName": {
						"value": "[parameters('OutputSql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}